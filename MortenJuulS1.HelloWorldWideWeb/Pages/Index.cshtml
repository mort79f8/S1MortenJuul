@page
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title></title>
</head>
<body>
    <h1>hello World</h1>
    <p>Det er i dag d.@DateTime.Now.ToLongDateString() og klokken er @DateTime.Now.ToShortTimeString()</p>
    @{
        Random rngNumber = new Random();
        int result = rngNumber.Next(1, 7);
    }
    <p>Du slår: @result med en 6 side terning.</p>
    @{
        for (int i = 0; i < result; i++)
        {
            <img src="~/img/565279.png" />
        }
    }
    @{ 
        List<string> names = new List<string>() { "Kian", "Anton", "Markus", "Morten" };
            <ul>
                @for (int i = 0; i < names.Count; i++)
                {
                    <li>@names[i]</li>
                }
            </ul>
    }
</body>
</html>